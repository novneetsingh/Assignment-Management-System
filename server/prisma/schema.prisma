generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum AccountType {
  Student
  Professor
}

model User {
  id          String      @id @default(uuid())
  name        String
  email       String      @unique
  password    String
  accountType AccountType @default(Student)
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
}
